{% extends 'base.html.twig' %}

{% block title %}AdminTech index{% endblock %}

{% block body %}
    <h1>AdminTech index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Email</th>
                <th>Image</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for admin_tech in admin_teches %}
            <tr>
                <td>{{ admin_tech.id }}</td>
                <td>{{ admin_tech.name }}</td>
                <td>{{ admin_tech.email }}</td>
                <td>{{ admin_tech.image }}</td>
                <td>
                    <a href="{{ path('app_admin_tech_show', {'id': admin_tech.id}) }}">show</a>
                    <a href="{{ path('app_admin_tech_edit', {'id': admin_tech.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_admin_tech_new') }}">Create new</a>
{% endblock %}
